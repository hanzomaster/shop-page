{
  "parser": "@babel/eslint-parser",
  "extends": [
    "plugin:prettier/recommended"
  ],
  "rules": {
    "linebreak-style": 0,
    "prettier/prettier": [
      1,
      {
        "endOfLine": "auto"
      }
    ]
  },
  "parserOptions": {
    "sourceType": "module",
    "babelOptions": {
      "presets": [
        "@babel/preset-react"
      ]
    },
    "requireConfigFile": false
  }
}
